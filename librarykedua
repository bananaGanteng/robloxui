Library:AddLabel("Home", "ğŸ“Œ Script by YourName")
Library:AddLabel("Home", "â­ Version 2.0.0")

Library:AddLabel("Player", "â”â”â” Movement Settings â”â”â”")
Library:AddLabel("Game", "â”â”â” Visual Settings â”â”â”")
Library:AddLabel("Misc", "â”â”â” Extra Features â”â”â”")

-- ================================================
-- TIPS & TRICKS
-- ================================================

--[[
    TIPS PENGGUNAAN:
    
    1. Tab yang tersedia: "Home", "Player", "Game", "Misc"
    
    2. AddToggle parameters:
       - Tab name (string)
       - Text (string)
       - Default state (boolean)
       - Callback function(state)
    
    3. AddSlider parameters:
       - Tab name (string)
       - Text (string)
       - Min value (number)
       - Max value (number)
       - Default value (number)
       - Callback function(value)
    
    4. AddButton parameters:
       - Tab name (string)
       - Text (string)
       - Callback function()
    
    5. AddLabel parameters:
       - Tab name (string)
       - Text (string)
    
    CONTOH KOMBINASI:
    
    -- Auto Farm dengan Speed Control
    local farmSpeed = 50
    
    Library:AddToggle("Game", "Auto Farm", false, function(state)
        _G.Farming = state
        while _G.Farming do
            wait(1/farmSpeed)
            -- Farm code here
        end
    end)
    
    Library:AddSlider("Game", "Farm Speed", 1, 100, 50, function(value)
        farmSpeed = value
    end)
]]

print("â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”")
print("Universal X - Custom Examples Loaded!")
